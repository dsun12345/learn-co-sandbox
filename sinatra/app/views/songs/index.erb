<h2>Welcome, <%=@user.name%></h2>	

<h3>All Songs</h3>	
<ul>	
  <% @songs.each do |song| %>	
    <li>	
      <%= song.id%>. <a href='/songs/<%=song.id%>'><%= song.name  %></a>, <%= song.artist%>, <%= song.genre %>, <%= song.release_date %>	
    </li>	
  <% end %>	
</ul>	

<a href='/songs/new'><button>Add to Song List</button></a> 

